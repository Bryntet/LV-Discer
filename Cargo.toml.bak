[package]
name = "tjing"
version = "0.1.0"
edition = "2021"

# See more keys and their definitions at https://doc.rust-lang.org/cargo/reference/manifest.html

[dependencies]
graphql_client = "0.12.0"
serde = "1.0.154"
clap = "4.1.8"
anyhow = "1.0.69"
[dependencies.reqwest]
version = "0.11.4"
features = ["json", "blocking"]
[dependencies.chrono]
version = "0.4.23"
features = ["serde"]
[dependencies.tokio]
version = "*"
features = ["full"]